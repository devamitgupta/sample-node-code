<div class="loader">
  <img src="img/loader.svg" />
</div>
<section class="pricing-outer">
  <div class="wrapper">
    <div class="steps-ticker">
      <%= __('PRICINGPAGE.TITLE') %>
    </div>
    <div class="steps-follow">
      <span>1</span>
      <%= __('PRICINGPAGE.STEP_1.TITLE') %>
    </div>
    <div class="step1">
      <ul class="sel-country-type">
        <li target="priceStep">
          <img src="img/kw.png">
          <span id="kw">
            <%= __('KUWAIT') %>
          </span>
        </li>
        <li target="priceStep">
          <img src="img/ksa.png">
          <span id="ksa">
            <%= __('SA') %>
          </span>
        </li>
      </ul>
    </div>
    <div class="step2" id="priceStep">
      <div class="steps-follow">
        <span>2</span>
        <%= __('PRICINGPAGE.STEP_2.TITLE') %>
      </div>
      <div class="duration-sel">
        <span>
          <%= __('PRICINGPAGE.STEP_2.DURATION_SELECT_TEXT') %>
        </span>
        <select id="duration-selector">
          <option value="31">
            <%= __('PRICINGPAGE.STEP_2.1_MONTH') %>
          </option>
          <option value="93">
            <%= __('PRICINGPAGE.STEP_2.3_MONTH') %>
          </option>
          <option value="184">
            <%= __('PRICINGPAGE.STEP_2.6_MONTH') %>
          </option>
          <option value="366">
            <%= __('PRICINGPAGE.STEP_2.1_YEAR') %>
          </option>
        </select>
      </div>
      <div class="pro-pricing-main">
        <ul class="leftspecial-pro">
        </ul>
      </div>
    </div>
    <div class="step3" id="payment">
      <div class="steps-follow">
        <span>3</span>
        <%= __('PRICINGPAGE.STEP_3.TITLE') %>
      </div>
      <div class="payment-method">
        <ul class="sel-country-type">
          <li>
            <img src="img/credit-card.svg">
            <span>
              <%= __('CREDITCARD') %>
            </span>
          </li>
          <li class="knet">
            <img src="img/logo.png">
            <span>
              <%= __('KNET') %>
            </span>
          </li>
        </ul>
      </div>
    </div>
  </div>
  <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-body login-form">
          <div class="loginlogoleft">
            <div class="loginlogo">
              <img src="img/logo.png">
            </div>
            <div class="login-heading">Login</div>
          </div>
          <form role="form" id="loginForm">
            <div class="error-bar"></div>
            <div class="form-group">
              <label for="usrname"> Username</label>
              <input type="text" class="form-control" name="username" placeholder="Enter email">
            </div>
            <div class="form-group">
              <label for="psw">Password</label>
              <input type="text" class="form-control" name="password" placeholder="Enter password">
            </div>
            <button type="submit" class="btn btn-success btn-block">Login</button>
          </form>
        </div>
      </div>
    </div>
  </div>
</section>
<input id="lang" type="hidden" value="<%= lang%>" />
<input id="url" type="hidden" value="<%= url%>" />